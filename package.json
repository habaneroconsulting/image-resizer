    "@vitejs/plugin-react-swc": "3.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-gtm-module": "2.0.11",
    "react-image-crop": "11.0.5",
    "react-router-dom": "6.21.3",
    "react-uid": "2.3.3",
    "vite": "5.0.12",
    "vite-plugin-svgr": "4.2.0",
