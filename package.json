    "prettier": "^3.2.4",
    "source-map-explorer": "2.5.3",
    "typescript": "5.3.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "tsc && vite build",
    "docker-test-build": "docker build -t playwright-tests .",
    "docker-test-run": "docker run -v ./test-downloads:/app/test-downloads -v ./playwright-report:/app/playwright-report playwright-tests",
